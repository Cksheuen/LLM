{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    {
      "identifier": "core:default",
      "allow": [
        {
          "url": "https://*.tauri.app"
        }
      ],
      "deny": [
        {
          "url": "https://private.tauri.app"
        }
      ]
    }
  ]
}