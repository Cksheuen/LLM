{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://api.coze.cn/$1"
        },
        {
            "source": "/authorisation/(.*)",
            "destination": "https://www.coze.cn/api/$1"
        },
        {
            "source": "/sign/(.*)",
            "destination": "https://www.coze.cn/sign/$1"
        }
    ]
}